import java.awt.EventQueue;
import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

import javax.swing.JFrame;
import java.awt.CardLayout;
import javax.swing.JPanel;
import javax.swing.JButton;

public class GUI {

	private JFrame frame;

	/**
	 * Launch the application.
	 */
	public static void main(String[] args) {
		EventQueue.invokeLater(new Runnable() {
			public void run() {
				try {
					GUI window = new GUI();
					window.frame.setVisible(true);
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
		});
	}

	/**
	 * Create the application.
	 */
	public GUI() {
		initialize();
	}

	/**
	 * Initialize the contents of the frame.
	 */
	private void initialize() {
		frame = new JFrame();
		frame.getContentPane().setLayout(new CardLayout(0, 0));
		
		final JPanel HomePage = new JPanel();
		frame.getContentPane().add(HomePage, "name_11891297357392");
		HomePage.setLayout(null);
		
		final JPanel StockManager = new JPanel();
		frame.getContentPane().add(StockManager, "name_11893316052823");
		StockManager.setLayout(null);
		
		final JPanel SalesManager = new JPanel();
		frame.getContentPane().add(SalesManager, "name_11894877285838");
		SalesManager.setLayout(null);
		
		JButton btnNewButton = new JButton("New button");
		btnNewButton.setBounds(39, 24, 89, 23);
		HomePage.add(btnNewButton);
		btnNewButton.addActionListener(new ActionListener(){
			public void actionPerformed(ActionEvent arg0){
				StockManager.setVisible(true);
				HomePage.setVisible(false);
			}
		});
		
		JButton btnNewButton_1 = new JButton("New button");
		btnNewButton_1.setBounds(39, 84, 89, 23);
		HomePage.add(btnNewButton_1);
		
		JButton btnNewButton_2 = new JButton("New button");
		btnNewButton_2.setBounds(39, 149, 89, 23);
		HomePage.add(btnNewButton_2);
		

		
		JButton btnNewButton_3 = new JButton("New button");
		btnNewButton_3.setBounds(31, 37, 89, 23);
		StockManager.add(btnNewButton_3);
		
		JButton btnNewButton_4 = new JButton("New button");
		btnNewButton_4.setBounds(181, 37, 89, 23);
		StockManager.add(btnNewButton_4);
		
		JButton btnNewButton_5 = new JButton("New button");
		btnNewButton_5.setBounds(318, 37, 89, 23);
		StockManager.add(btnNewButton_5);
		
		JButton btnNewButton_6 = new JButton("New button");
		btnNewButton_6.setBounds(10, 36, 89, 23);
		SalesManager.add(btnNewButton_6);
		
		JButton btnNewButton_7 = new JButton("New button");
		btnNewButton_7.setBounds(335, 227, 89, 23);
		SalesManager.add(btnNewButton_7);
	}
}
