import java.util.ArrayList;
import java.util.List;

//package SalesOrderlineRecord;
public class SalesOrderlineRecord {
	private int saleId;
	private String date;
	private int itemId;
	private double price;
	private int qty;
	public SalesOrderlineRecord (int saleId, String date, int itemId, double price, int qty)
	{
		this.saleId = saleId;
		this.date = date;
		this.itemId = itemId;
		this.price = price;
		this.qty = qty;
	}

	public int getSaleId ()
	{
		return this.saleId;
	}
	
	public String getDate ()
	{
		return this.date;
	}

	public int getItemId ()
	{
		return this.itemId;
	}

	public double getPrice ()
	{
		return this.price;
	}

	public int getQty ()
	{
		return this.qty;
	}
}
